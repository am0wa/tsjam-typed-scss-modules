{
  "name": "@tsjam/typed-scss-modules",
  "version": "8.2.0-7",
  "description": "TypeScript type definition generator for SCSS CSS Modules",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "author": "am0wa <am0wa.it@gmail.com>",
  "contributors": [
    {
      "name": "Spencer Miskoviak",
      "email": "smiskoviak@gmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.19",
    "npm": ">=10.9"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/am0wa/tsjam-typed-scss-modules.git"
  },
  "homepage": "https://github.com/am0wa/tsjam-typed-scss-modules#readme",
  "keywords": [
    "scss",
    "css modules",
    "cli",
    "typescript",
    "type generator",
    "scss modules"
  ],
  "files": [
    "dist/lib"
  ],
  "bin": "./dist/lib/cli.js",
  "scripts": {
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "typed-scss-modules": "ts-node ./lib/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc && chmod +x dist/lib/cli.js",
    "check:types": "tsc --noEmit",
    "check:formatting": "prettier --check '**/*.{js,json,css,md,scss,tsx,ts}'",
    "check:linting": "eslint .",
    "commit": "commit",
    "prepare": "npm run build && husky",
    "prepublishOnly": "[ \"$CI\" = \"true\" ] || (npm run build && npm test)",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@tsjam/eslint-config-recommended": "^0.2.4",
    "@types/glob": "^7.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^17.0.18",
    "@types/prettier": "^2.7.3",
    "@types/reserved-words": "^0.1.0",
    "@types/yargs": "^17.0.8",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "eslint": "^9.34.0",
    "eslint-plugin-jest": "^29.11.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sass": "^1.77.7",
    "sass-embedded": "^1.77.7",
    "semantic-release": "^17.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "sass": "^1.77.7",
    "sass-embedded": "^1.77.7"
  },
  "peerDependenciesMeta": {
    "sass": {
      "optional": true
    },
    "sass-embeded": {
      "optional": true
    }
  },
  "dependencies": {
    "bundle-require": "^4.0.4",
    "chalk": "4.1.2",
    "change-case": "^4.1.2",
    "chokidar": "^3.5.3",
    "esbuild": "^0.27.2",
    "glob": "^13.0.0",
    "joycon": "^3.1.1",
    "postcss": "^8.4.27",
    "postcss-modules": "^6.0.0",
    "reserved-words": "^0.1.2",
    "slash": "^3.0.0",
    "yargs": "^17.3.1"
  },
  "lint-staged": {
    "*.{js,json,css,md,scss,tsx,ts}": [
      "prettier --write"
    ]
  }
}
